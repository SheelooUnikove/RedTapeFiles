    <script src="js/jquery.cbpFWSlider.min.js"></script>
    <script>
        $(function () {

            $('#cbp-fwslider').cbpFWSlider();

            setInterval(function () {
                if ($('.cbp-fwnext').is(":visible")) {
                    $('.cbp-fwnext').click();
                }
                else {
                    $('.cbp-fwdots').find('span').click();
                }
            }, 6000);

        });
		</script>

    <script type="text/javascript">

        $(window).load(function () {

            var rt_size = $(".life_slidr ul li").size();
            var cur_product_view = 1;
            var cur_img = $(".life_slidr ul li:nth-child(" + cur_product_view + ")").children("div").children("img").attr('src');
            $(".main_img_camp").attr('src', cur_img);

            // $("#flexiselDemo1").flexisel();
            $("#flexiselDemo1").flexisel({
                enableResponsiveBreakpoints: true,
                responsiveBreakpoints: {
                    portrait: {
                        changePoint: 480,
                        visibleItems: 1
                    },
                    landscape: {
                        changePoint: 640,
                        visibleItems: 2
                    },
                    tablet: {
                        changePoint: 768,
                        visibleItems: 3
                    },
                    pc: {
                        changePoint: 2560,
                        visibleItems: 3
                    },

                }
            });


            var cur_product_view = 1;

            $(".nbs-flexisel-item").click(function () {


                var li_index = $(this).index();

                var cur_img = $(this).children("div").children("img").attr('src');
                li_index = li_index + 1;
                cur_product_view = li_index;
                $(".main_img_camp").attr('src', cur_img);
                $(".nbs-flexisel-item").removeClass("selected");
                $(this).addClass("selected");
                //change_zoom_btn(cur_product_view+2);



            });


            $(".main_prod_left").click(function () {


                if (cur_product_view == 1) {
                    cur_product_view = rt_size;
                }
                else {
                    cur_product_view = cur_product_view - 1;
                }
                var cur_img = $(".life_slidr .nbs-flexisel-item:nth-child(" + cur_product_view + ")").children("div").children("img").attr('src');
                $(".main_img_camp").attr('src', cur_img);
            });


            $(".main_prod_right").click(function () {


                if (cur_product_view == rt_size) {
                    cur_product_view = 1;
                }
                else {
                    cur_product_view = cur_product_view + 1;
                }
                var cur_img = $(".life_slidr .nbs-flexisel-item:nth-child(" + cur_product_view + ")").children("div").children("img").attr('src');
                $(".main_img_camp").attr('src', cur_img);
            });



            $(".nbs-flexisel-nav-left").css({ "margin-top": 0, "top": "50%", "left":"10px" });

            $(".nbs-flexisel-nav-right").css({ "margin-top": 0, "top": "50%", "right": "-16px" });


        });
</script>
