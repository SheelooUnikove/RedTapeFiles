            <script>
                $(function () {

                    //ProductCats.GetCategoriesList(2,success, fail);
                    RedTapeWeb.Services.ProductCats.GetCategoriesList(0, success, fail);
                });


                function success(results) {

                    $("#ulcollections").html('');

                    for (var i = 0; i < results.length; i++) {
                        if (results[i].CategoryId == '0') {
                            $("#ulcollections").append("<li class='sub_nav_cat'><ul class='dv" + results[i].SubCategoryId + "'><li class='in_sub_nv'> <a href='ProductsList.aspx?Category=" + results[i].SubCategoryId + "&subcat=" + results[i].SubCategoryId + "'>" + results[i].SubCategory + "</a></li></ul></li>");
                        }
                        else {
                            //$(".dv" + results[i].CategoryId).append("<li onclick='gotoproductcat(" + results[i].CategoryId + "," + results[i].SubCategoryId + ")'>" + results[i].SubCategory + "</li>");
                            $(".dv" + results[i].CategoryId).append("<li><a href='ProductsList.aspx?Category=" + results[i].CategoryId + "&subcat=" + results[i].SubCategoryId + "'>" + results[i].SubCategory + "</a></li>");
                        }
                    }
                    $("#ulcollections").append("<li class='sub_nav_cat'><ul><li class='in_sub_nv'><a href='new_arrivals.aspx'>New Arrivals</a></li></ul></li>");

                    var nav_scroll_thresh = 40; // scroll compresses after this
                    var mobile_nav_thresh = 790; // when desktop design shifts to mobile
                    var nav_mobil_count = 0; //flag
                    var footer_mobile_nav = 0; //flag
                    var nav_mob_height = parseInt($(window).height()) - parseInt($("header").height()); // to fit the mobile nav same as the window


                    var sub_nav_flag = 1;
                    var sub_nav_width_init = $(".red_sub_nav_menu").width();

                    function set_menu_height(element) {
                        //console.log(sub_nav_flag);
                        sub_nav_flag = sub_nav_flag + 1;
                        $(element).css({ width: sub_nav_flag * sub_nav_width_init });
                        $(element).children(".sub_nav_cat").css({ "width": 100 / sub_nav_flag + "%" });
                        if ($(window).width() > mobile_nav_thresh && $(element).height() > $(window).height() - $("header").height() && sub_nav_flag < 6) {
                            set_menu_height(element);
                        }
                    }

                    // function to be called after nav list is binded.

                    $(".red_sub_nav_menu").each(function (i, obj) {
                        //test
                        sub_nav_flag = 1;
                        $(this).children(".sub_nav_cat").css({ "width": "100%" });
                        if ($(window).width() > mobile_nav_thresh && $(this).height() > $(window).height() - $("header").height()) {
                            set_menu_height(this, sub_nav_flag);
                        }

                    });

                }
                function fail() {
                }
                function gotoproductcat(vals, subcat) {

                    window.location.href = 'ProductsList.aspx?Category=' + vals + '&subcat=' + subcat;
                }
            </script>
            
