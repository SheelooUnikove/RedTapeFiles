{

.product_detail_row
{
float:left;
width:67%;
}

.buy_now
{
width:100%;
}

}

@media screen and (max-width: 600px)
{

.login_btn ,  .or_you_txt
{
float:left;
margin-top:20px;
margin-bottom:20px;
}


}


.own_a_look
{
 /* Firefox */
height: -moz-calc(100% - 118px);
/* WebKit */
height: -webkit-calc(100% - 118px);
/* Opera */
height: -o-calc(100% - 118px);
/* Standard */
height: calc(100% - 118px);
}
.slick-slide
{
 background-size:cover;
}

.cbp-fwslider .own_look_btn
{
 width:130px;
 height:35px;
 background-color:#e0203f;
 position:absolute;
 top:70%;
 right:10%;
 color:#fff;
 font-size:11px;
 z-index:100;
 line-height:35px;
 text-align:left;
 padding-left:10px;
 
 font-family: 'GothamRoundedBold';
}

.slick-dots
{
 height:27px!important;
 bottom:10px!important;
}
.cbp-fwslider ul li
{
 min-width:30px!important;
}
.slick-dots li button:before
{
 display: inline-block!important;
 width: 14px!important;
 height: 14px!important;
 background: #ddd!important;
 margin: 4px!important;
 border-radius: 50%!important;
 cursor: pointer!important;
 font-size:0px !important;
 opacity:1;
}
.slick-dots li.slick-active button:before
{
 background:#e92629!important;
}
.slick-dots
{
 right:0px!important;
 width:100px!important;
}
.product_nav_btn
{
 background-image:url(../images/website/RT_slide-in_icon.jpg);
}
.nbs-flexisel-container
{
 float:left;
}
.rt_life_img_cont
{
 position:relative;
}

.rt_life_img_cont .main_prod_left,.rt_life_img_cont .main_prod_right
{
 top:50%;
}
.rt_life_img_cont .main_prod_left
{
 left:15px;
}
    .rt_life_img_cont .main_prod_right {
    }
#ctl00_SiteMap1 span
{
 margin:2px !important;
 font-size:11px;
}

