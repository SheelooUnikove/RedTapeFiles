.product_nav_btn
{
	position:fixed;
	left:80%;
	z-index:10;
	display:none;
	width:40px;
	height:40px;
	background:red;
	margin-top:0px;
	cursor:pointer;
}

@media screen and (max-width:960px) {
	.product_listing_nav
	{
		right: -195px;
	}
	.product_listing_nav.open
	{
			right:0px;
	}
	.cart_net,.cart_total,.cart_cntun_shpng,.cart_plc_order
	{
	font-size:18px;		
	}
	
		
}
@media screen and (max-width:800px) {
	
	.cart_net,.cart_total,.cart_cntun_shpng,.cart_plc_order
	{
	font-size:14px;		
	}
}
@media screen and (max-width:790px) {
header nav ul li.search_box input /* just for now */
{
	display:none;
}
.product_nav_btn
	{
		display:block;
	}
	.product li .add_wishlist_pop ,.product li .add_wishlist_pop.open
	{
		bottom:49px;
	}
}


@media screen and (max-width: 700px)

{
	.shpng_cart ul li:nth-child(1)
	{
		display:none;
	}
	.shpng_cart ul li ul li:nth-child(1)
	{
		display:block;
	}
	.shpng_cart ul li ul li
	{
		width:25%;
		text-align: center;
margin-top: 10px;
	}
	.shpng_cart ul li ul li:nth-child(1)
	{
		width:100%;
	}
	.shpng_cart ul li ul li:nth-child(1) img
	{
		
position: relative;
	}
	.shpng_cart ul li ul li:nth-child(2)
	{
		width: 100%;
text-align: center;
margin: 10px 0px 10px 0px;
padding: 0px;
	}
}



@media screen and (max-width: 656px)
{
	.pair_up .nbs-flexisel-nav-left, .pair_up .nbs-flexisel-nav-right
	{
		display:block;
	}
	
}

@media screen and (max-width: 650px)
{
.main_prod_left {
left:5px;
}
.main_prod_right
{
	right:5px;
}
.color_option ul {
list-style: none;
left: 50%;
position: relative;
margin-left: -101px;
}
.color_option ul li {
width: 40px;
height: 40px;
}
.color_option
{
	width:100%;
}
.color_option ul li.selected {
background-position: 14px 12px;
}
.hlp_cntct
{
	width:100%;
	text-align:center;
}
.cart_cntun_shpng,.cart_plc_order
{
	width:50%;
	margin-left:0px;
}
}

@media screen and (max-width:600px) {
	
	.product li .add_wishlist_pop ,.product li .add_wishlist_pop.open
	{
		bottom:64px;
		width:90%;
        left:initial;
	}
	.product li
	{
		width:100%;
	}
}

@media screen and (max-width:500px) {
	footer {
height: 125px;
}
	
	.debits
	{
		top:37px;
	}
	.logo
	{
width: 92px;
margin-top: 19px;
background-size: 100%;
background-repeat: no-repeat;
	}
	.product_nav_btn
	{
		left:80%;
	}
	.cart_cntun_shpng,.cart_plc_order
{
	width:100%;
	margin-top:10px;
}
}
@media screen and (max-width:380px) {
	.product li .add_wishlist_pop ,.product li .add_wishlist_pop.open
	{
		bottom:54px;
	}
}

@media screen and (max-width:320px) {
	.produc_tag_1, .produc_tag_2, .produc_tag_3 {
right: 11px;
top: 12px;
}

	
}



.look_pointer_line , .look_pointer_info , .look_pointer_redirect
{
display:none;
}

